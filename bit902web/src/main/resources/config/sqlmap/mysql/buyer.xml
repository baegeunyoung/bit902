<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.smartpayweb.repository.mapper.BuyerMapper">

 <!-- ===================== 예약정보 ===================== --> 
<resultMap type="buyerVO" id="buyerMap">
 	<result column="buyer_no" property="buyerNo"/>
 	<result column="id" property="id"/>
 	<result column="password" property="password"/>
 	<result column="name" property="name"/>
 	<result column="cellphone_number" property="cellphoneNumber"/>
</resultMap>
 <!-- 예약시간 체크하기 -->
<select id="selectBuyer" resultMap="buyerMap">
	select * 
	from tb_buyer
</select> 

</mapper>











